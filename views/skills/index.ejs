<!DOCTYPE html>
<html>
  <head>
    <title>Express Dev Skills</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Skills</h1>
    <ul class="list">
      <%skills.forEach(function(s){%>
        <li>
          <a href="/skills/<%= s.id %>"><%= s.skill %></a>
        </li>
      <% }); %>
    </ul>
  </body>

</html>
